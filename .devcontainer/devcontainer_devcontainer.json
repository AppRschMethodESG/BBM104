{
    "name": "R Dev Container",
    "build": {
        "dockerfile": "Dockerfile",
        "context": ".."
    },
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    },
    "extensions": [
        "REditorSupport.r",
        "ms-python.python"
    ],
    "postCreateCommand": "R -e \"install.packages('languageserver', repos='http://cran.rstudio.com/')\"",
    "remoteUser": "vscode",
    "features": {}
}